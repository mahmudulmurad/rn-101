{
  "expo": {
    "name": "PlayNative",
    "slug": "PlayNative",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./apps/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./apps/assets/lvb.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./apps/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PlayNative to use your location."
        }
      ],
      "expo-secure-store"
    ]
  }
}
